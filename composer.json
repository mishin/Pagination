{
    "name": "ucsdmath/pagination",
    "type": "library",
    "description": "Simple pagination",
    "keywords": ["pagination", "ucsdmath", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2015-12-05",
    "authors": [
        {"name": "Daryl Eisner", "email": "deisner@ucsd.edu"}
    ],
    "support": {
        "source": "https://github.com/UCSDMath/Pagination",
        "email": "deisner@ucsd.edu"
    },
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.4",
        "phpunit/phpunit": "5.2.3",
        "ucsdmath/testing": "1.6.x-dev",
        "squizlabs/php_codesniffer": "2.5.1"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Pagination\\": "src/Pagination/",
            "UCSDMath\\Pagination\\Exception\\": "src/Pagination/Exception/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        }
    }
}
